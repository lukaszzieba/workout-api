{
  "name": "workout",
  "version": "0.0.1",
  "description": "Sotrk's wokout",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --files src/index.ts",
    "test": "NODE_ENV=test jest --testTimeout=5000",
    "seed:make": "knex seed:make new_seed --knexfile src/db/knexfile.ts --client pg",
    "seed:run": "knex seed:run --knexfile src/db/knexfile.ts --client pg",
    "migration:make": "knex migrate:make new_migration --knexfile src/db/knexfile.ts --client pg",
    "migration:up": "knex migrate:up --knexfile src/db/knexfile.ts --client pg",
    "migration:down": "knex migrate:down --knexfile src/db/knexfile.ts --client pg"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.30.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.3",
    "joi": "^17.6.3",
    "knex": "^2.3.0",
    "knex-stringcase": "^1.4.6",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.18",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.0",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.8.4",
    "yarn-upgrade-all": "^0.7.1"
  }
}
